<?xml version="1.0" encoding="utf-8"?>
<android.support.v7.widget.GridLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/apk/res-auto"
    android:id="@+id/grid_UI"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:columnCount="4"
    tools:orientation="horizontal"
    tools:rowCount="23">

    <Button
        android:id="@+id/button15"
        android:layout_width="82dp"
        android:layout_height="wrap_content"
        android:text="写"
        tools:layout_column="3"
        tools:layout_row="5" />

    <Button
        android:id="@+id/button14"
        android:layout_width="82dp"
        android:layout_height="wrap_content"
        android:text="写"
        tools:layout_column="3"
        tools:layout_row="6" />

    <Button
        android:id="@+id/button13"
        android:layout_width="82dp"
        android:layout_height="wrap_content"
        android:text="写"
        tools:layout_column="3"
        tools:layout_row="4" />

    <Button
        android:id="@+id/button12"
        android:layout_width="82dp"
        android:layout_height="wrap_content"
        android:text="写"
        tools:layout_column="3"
        tools:layout_row="3" />

    <Button
        android:id="@+id/button4"
        android:layout_width="82dp"
        android:layout_height="wrap_content"
        android:text="写"
        tools:layout_column="3"
        tools:layout_row="2" />

    <Button
        android:id="@+id/button10"
        android:layout_width="82dp"
        android:layout_height="wrap_content"
        android:text="读"
        tools:layout_column="2"
        tools:layout_row="6" />

    <Button
        android:id="@+id/button9"
        android:layout_width="82dp"
        android:layout_height="wrap_content"
        android:text="读"
        tools:layout_column="2"
        tools:layout_row="5" />

    <Button
        android:id="@+id/button8"
        android:layout_width="82dp"
        android:layout_height="wrap_content"
        android:text="读"
        tools:layout_column="2"
        tools:layout_row="4" />

    <Button
        android:id="@+id/button7"
        android:layout_width="82dp"
        android:layout_height="wrap_content"
        android:text="读"
        tools:layout_column="2"
        tools:layout_row="3" />

    <Button
        android:id="@+id/button3"
        android:layout_width="82dp"
        android:layout_height="wrap_content"
        android:text="读"
        tools:layout_column="2"
        tools:layout_row="2" />

    <TextView
        android:id="@+id/textTitle1"
        android:layout_width="111dp"
        android:layout_height="wrap_content"
        android:background="#00BCD4"
        android:text="参数"
        android:textAlignment="center"
        android:textStyle="bold"
        tools:layout_column="0"
        tools:layout_row="0" />

    <TextView
        android:id="@+id/textTitle2"
        android:layout_width="130dp"
        android:layout_height="wrap_content"
        android:background="#00BCD4"
        android:backgroundTint="#00BCD4"
        android:text="赋值"
        android:textAlignment="center"
        android:textStyle="bold"
        tools:layout_column="1"
        tools:layout_row="0" />

    <TextView
        android:id="@+id/textTitle3"
        android:layout_width="164dp"
        android:layout_height="wrap_content"
        android:background="#00BCD4"
        android:text="读/写"
        android:textAlignment="center"
        android:textStyle="bold"
        tools:layout_column="2"
        tools:layout_columnSpan="2"
        tools:layout_gravity="fill"
        tools:layout_row="0" />

    <TextView
        android:id="@+id/textView0_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="用户密码"
        android:textColor="@android:color/background_dark"
        android:textStyle="bold"
        tools:layout_column="0"
        tools:layout_row="1" />

    <TextView
        android:id="@+id/textView0_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="控制方式选择"
        android:textColor="@android:color/background_dark"
        android:textStyle="bold"
        tools:layout_column="0"
        tools:layout_row="2" />

    <TextView
        android:id="@+id/textView0_3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="频率源选择"
        android:textColor="@android:color/background_dark"
        android:textStyle="bold"
        tools:layout_column="0"
        tools:layout_row="3" />

    <TextView
        android:id="@+id/textView0_4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="数字频率给定"
        android:textColor="@android:color/background_dark"
        android:textStyle="bold"
        tools:layout_column="0"
        tools:layout_row="4" />

    <TextView
        android:id="@+id/textView0_5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="运行命令选择"
        android:textColor="@android:color/background_dark"
        android:textStyle="bold"
        tools:layout_column="0"
        tools:layout_row="5" />

    <TextView
        android:id="@+id/textView0_6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="运行方向选择"
        android:textColor="@android:color/background_dark"
        android:textStyle="bold"
        tools:layout_column="0"
        tools:layout_row="6" />

    <EditText
        android:id="@+id/editText1_1"
        android:layout_width="130dp"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="0~65536"
        android:inputType="number"
        tools:layout_column="1"
        tools:layout_row="1" />

    <EditText
        android:id="@+id/editText1_2"
        android:layout_width="130dp"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="0~2"
        android:inputType="number"
        tools:layout_column="1"
        tools:layout_row="2" />

    <EditText
        android:id="@+id/editText1_3"
        android:layout_width="130dp"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="0~5"
        android:inputType="number"
        tools:layout_column="1"
        tools:layout_row="3" />

    <EditText
        android:id="@+id/editText1_4"
        android:layout_width="130dp"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="0~30000"
        android:inputType="number"
        tools:layout_column="1"
        tools:layout_row="4" />

    <EditText
        android:id="@+id/editText1_5"
        android:layout_width="130dp"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="0~2"
        android:inputType="number"
        tools:layout_column="1"
        tools:layout_row="5" />

    <EditText
        android:id="@+id/editText1_6"
        android:layout_width="130dp"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="0~1"
        android:inputType="number"
        tools:layout_column="1"
        tools:layout_row="6" />

    <Button
        android:id="@+id/button2"
        android:layout_width="82dp"
        android:layout_height="wrap_content"
        android:text="写"
        tools:layout_column="3"
        tools:layout_row="1" />

    <Button
        android:id="@+id/button1"
        android:layout_width="82dp"
        android:layout_height="wrap_content"
        android:text="读"
        tools:layout_column="2"
        tools:layout_row="1" />

</android.support.v7.widget.GridLayout>
